
public class Perfectnumber
{
public static void main (String[]args)
 {
	int num=6;
	 int i=1;
	 int sum=0;
	
	while (num>i)
	{
		if(num%i==0)
		{
		
			sum=sum+i;
		}
		i++;
	}
	if(sum==num)
	{
		System.out.println(num +" is perfect number");
	}
	else
	{
		System.out.println(num +" is not perfect number");
	}
 }
}
